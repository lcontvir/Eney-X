<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/PerfilUsuarioFondo"
    tools:context=".MainActivity"

    >

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/imageView2"
        android:layout_width="174dp"
        android:layout_height="173dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="28dp"
        android:scaleType="centerCrop"
        android:src="@drawable/PerfilUsuarioFotoCarnet"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:shapeAppearanceOverlay="@style/EstiloImagenCircular" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="184dp"
        android:layout_height="46dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="13dp"
        android:text="Martín"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="185dp"
        android:layout_height="43dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="12dp"
        android:text="23 años"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="184dp"
        android:layout_height="50dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="12dp"
        android:text="Buda Cristo"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <Button
        android:id="@+id/button6"
        style="@style/EstiloBotonRectangular"
        android:layout_width="184dp"
        android:layout_height="141dp"
        android:layout_marginEnd="15dp"
        android:text="Licencias"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        tools:layout_editor_absoluteY="228dp" />

    <Button
        android:id="@+id/button"
        style="@style/EstiloBotonRectangular"
        android:layout_width="184dp"
        android:layout_height="141dp"
        android:layout_marginEnd="14dp"
        android:layout_marginBottom="12dp"
        android:text="Privacidad"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/button11"
        app:layout_constraintEnd_toEndOf="parent" />


    <Button
        android:id="@+id/button8"
        style="@style/EstiloBotonRectangular"
        android:layout_width="184dp"
        android:layout_height="141dp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="12dp"
        android:text="Facturacion"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/button7"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.931" />

    <Button
        android:id="@+id/button7"
        style="@style/EstiloBotonRectangular"
        android:layout_width="184dp"
        android:layout_height="141dp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="80dp"
        android:text="Dispositivos"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/imageViewSeparator"
        app:layout_constraintEnd_toStartOf="@+id/button"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/botonCasita"
        android:layout_width="114dp"
        android:layout_height="90dp"
        android:layout_marginStart="14dp"
        android:layout_marginBottom="22dp"
        android:background="@android:color/transparent"
        android:contentDescription="Botón de casita"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:tint="@color/white"
        tools:src="@drawable/PerfilUsuarioCasa" />

    <ImageButton
        android:id="@+id/botonCasita7"
        android:layout_width="113dp"
        android:layout_height="90dp"
        android:layout_marginStart="149dp"
        android:layout_marginEnd="150dp"
        android:layout_marginBottom="22dp"
        android:background="@android:color/transparent"
        android:contentDescription="Botón de casita"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:tint="@color/white"
        tools:src="@drawable/PerfilUsuarioEscudo" />

    <ImageButton
        android:id="@+id/botonCasita8"
        android:layout_width="110dp"
        android:layout_height="92dp"
        android:layout_marginEnd="19dp"
        android:layout_marginBottom="22dp"
        android:background="@android:color/transparent"
        android:contentDescription="Botón de casita"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:tint="@color/white"
        tools:src="@drawable/PerfilUsuarioAjustes" />

    <Button
        android:id="@+id/button11"
        style="@style/EstiloBotonRectangular"
        android:layout_width="378dp"
        android:layout_height="60dp"
        android:layout_marginStart="18dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="24dp"
        android:background="#A7D397"
        android:text="Nosotros"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/botonCasita7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageViewSeparator"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:scaleType="fitXY"
        android:src="@android:drawable/divider_horizontal_bright"
        tools:layout_editor_absoluteX="-28dp"
        tools:layout_editor_absoluteY="604dp" />

    <ImageView
        android:id="@+id/imageViewSeparator3"
        android:layout_width="0.7dp"
        android:layout_height="150dp"
        android:layout_marginStart="136dp"
        android:background="@color/white"
        android:scaleType="fitXY"
        android:src="@android:drawable/divider_horizontal_bright"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="604dp" />

    <ImageView
        android:id="@+id/imageViewSeparator4"
        android:layout_width="0.6dp"
        android:layout_height="150dp"
        android:layout_marginStart="272dp"
        android:background="@color/white"
        android:scaleType="fitXY"
        android:src="@android:drawable/divider_horizontal_bright"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="604dp" />


</androidx.constraintlayout.widget.ConstraintLayout>


