<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#252627"
    android:backgroundTint="@color/background"
    tools:context=".MainActivity">

    <View
        android:id="@+id/navvar"
        android:layout_width="411dp"
        android:layout_height="81dp"
        android:background="@drawable/navvar_bloque"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/botonCheck"
        android:layout_width="340dp"
        android:layout_height="57dp"
        android:layout_marginStart="37dp"
        android:layout_marginTop="540dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="53dp"
        android:backgroundTint="@color/botonVerde"
        android:text="@string/textoBotonVerde"
        android:textColor="@color/Blanco"
        android:textSize="18dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/navvar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/botonAjustes"
        android:layout_width="136dp"
        android:layout_height="60dp"
        android:layout_marginBottom="8dp"
        android:background="@android:color/transparent"
        android:contentDescription="Boton Ajustes"
        android:scaleType="fitCenter"
        android:src="@drawable/ajustes"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/botonEscudo"
        app:tint="@color/Blanco" />

    <ImageButton
        android:id="@+id/botonCasa"
        android:layout_width="135dp"
        android:layout_height="60dp"
        android:layout_marginBottom="8dp"
        android:background="@android:color/transparent"
        android:contentDescription="Boton casa"
        android:scaleType="fitCenter"
        android:src="@drawable/casa"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/botonEscudo"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/botonEscudo"
        android:layout_width="136dp"
        android:layout_height="60dp"
        android:layout_marginBottom="8dp"
        android:background="@android:color/transparent"
        android:contentDescription="Boton Escudo"
        android:scaleType="fitCenter"
        android:src="@drawable/escudo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/botonAjustes"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/botonCasa" />

    <ImageView
        android:id="@+id/imagenCheck"
        android:layout_width="178dp"
        android:layout_height="201dp"
        android:layout_marginStart="117dp"
        android:layout_marginTop="73dp"
        android:layout_marginEnd="116dp"
        android:layout_marginBottom="457dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/escudo" />

    <TextView
        android:id="@+id/textoCheckIn"
        android:layout_width="296dp"
        android:layout_height="50dp"
        android:layout_marginStart="58dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="57dp"
        android:text="@string/textoCheckIn"
        android:textColor="@color/Blanco"
        android:textSize="35dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imagenCheck" />

    <TextView
        android:id="@+id/textoCheckInColor"
        android:layout_width="298dp"
        android:layout_height="41dp"
        android:layout_marginStart="57dp"
        android:layout_marginTop="7dp"
        android:layout_marginEnd="56dp"
        android:text="@string/textoCheckInColor"
        android:textAlignment="center"
        android:textColor="@color/botonVerde"
        android:textSize="35dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textoCheckIn" />

    <TextView
        android:id="@+id/textoVPN"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:layout_marginBottom="11dp"
        android:text="@string/VPNCheck"
        android:textColor="@color/Blanco"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textoContrasena"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textoContrasena"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:layout_marginBottom="12dp"
        android:text="@string/ContraseÃ±as"
        android:textColor="@color/Blanco"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textoProtectorWeb"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textoProtectorWeb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="45dp"
        android:layout_marginBottom="38dp"
        android:text="@string/ProtectorWeb"
        android:textColor="@color/Blanco"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/botonCheck"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textoAntiPublicidad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="5dp"
        android:layout_marginBottom="6dp"
        android:text="@string/Antipublicidad"
        android:textColor="@color/Blanco"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textoControlParental"
        app:layout_constraintEnd_toStartOf="@+id/circuloVerde4" />

    <TextView
        android:id="@+id/textoControlParental"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="5dp"
        android:layout_marginBottom="9dp"
        android:text="@string/ControlParental"
        android:textColor="@color/Blanco"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textoKrilia"
        app:layout_constraintEnd_toStartOf="@+id/circuloVerde5" />

    <TextView
        android:id="@+id/textoKrilia"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="5dp"
        android:layout_marginBottom="38dp"
        android:text="@string/Krilia"
        android:textColor="@color/Blanco"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/botonCheck"
        app:layout_constraintEnd_toStartOf="@+id/circuloNaranja1" />

    <ImageView
        android:id="@+id/circuloVerde1"
        android:layout_width="12dp"
        android:layout_height="16dp"
        android:layout_marginStart="5dp"
        android:layout_marginBottom="15dp"
        android:src="@drawable/circuloverde"
        app:layout_constraintBottom_toTopOf="@+id/circuloVerde2"
        app:layout_constraintStart_toEndOf="@+id/textoVPN"
        app:srcCompat="@drawable/circuloverde" />

    <ImageView
        android:id="@+id/circuloVerde2"
        android:layout_width="12dp"
        android:layout_height="16dp"
        android:layout_marginStart="5dp"
        android:layout_marginBottom="15dp"
        android:src="@drawable/circuloverde"
        app:layout_constraintBottom_toTopOf="@+id/circuloVerde3"
        app:layout_constraintStart_toEndOf="@+id/textoContrasena"
        app:srcCompat="@drawable/circuloverde" />

    <ImageView
        android:id="@+id/circuloVerde3"
        android:layout_width="12dp"
        android:layout_height="16dp"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="38dp"
        android:src="@drawable/circuloverde"
        app:layout_constraintBottom_toTopOf="@+id/botonCheck"
        app:layout_constraintStart_toEndOf="@+id/textoProtectorWeb"
        app:srcCompat="@drawable/circuloverde" />

    <ImageView
        android:id="@+id/circuloVerde4"
        android:layout_width="12dp"
        android:layout_height="16dp"
        android:layout_marginEnd="50dp"
        android:layout_marginBottom="10dp"
        android:src="@drawable/circuloverde"
        app:layout_constraintBottom_toTopOf="@+id/circuloVerde5"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/circuloverde" />

    <ImageView
        android:id="@+id/circuloVerde5"
        android:layout_width="12dp"
        android:layout_height="16dp"
        android:layout_marginEnd="42dp"
        android:layout_marginBottom="12dp"
        android:src="@drawable/circuloverde"
        app:layout_constraintBottom_toTopOf="@+id/circuloNaranja1"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/circuloverde" />

    <ImageView
        android:id="@+id/circuloNaranja1"
        android:layout_width="12dp"
        android:layout_height="16dp"
        android:layout_marginEnd="114dp"
        android:layout_marginBottom="38dp"
        android:src="@drawable/circulonaranja"
        app:layout_constraintBottom_toTopOf="@+id/botonCheck"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/circulonaranja" />

</androidx.constraintlayout.widget.ConstraintLayout>