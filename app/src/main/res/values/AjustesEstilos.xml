<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="EstiloBotonNaranja">
        <item name="android:backgroundTint">@color/amarillito</item>
        <item name="android:textColor">#3A3737</item>
        <item name="android:textSize">18dp</item>
    </style>

    <style name="EstiloBotonVerde">
        <item name="android:backgroundTint">#71DF4A</item>
        <item name="android:textColor">#3A3737</item>
        <item name="android:textSize">18dp</item>
    </style>
    <style name="EstiloBotonRectangularAzul">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="shapeAppearance">@id/rectangles</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:backgroundTint">@color/azulito</item>
        <item name="android:textSize">16sp</item>
    </style>


</resources>